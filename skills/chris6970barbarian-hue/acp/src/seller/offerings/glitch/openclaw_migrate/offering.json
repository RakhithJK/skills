{
  "name": "openclaw_migrate",
  "description": "Migrate OpenClaw from one host to another via SSH. Automatically syncs all config, skills, memory, tokens, environment variables, and cron jobs to a new machine with one command.",
  "jobFee": 1.0,
  "jobFeeType": "fixed",
  "requiredFunds": false,
  "requirement": {
    "type": "object",
    "properties": {
      "action": {
        "type": "string",
        "description": "Action: migrate, setup, test"
      },
      "targetHost": {
        "type": "string",
        "description": "Target host IP or hostname"
      },
      "sshUser": {
        "type": "string",
        "description": "SSH username (default: crix)"
      }
    },
    "required": ["action"]
  }
}
